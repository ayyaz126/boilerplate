datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // ðŸ‘ˆ ye line must hai Prisma v6 me
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

